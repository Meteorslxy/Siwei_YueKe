/* 清除浮动 */
.clearfix::after {
  content: "";
  display: block;
  clear: both;
}
/* 滚动条 */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
/* 图片加载错误处理 */
image {
  will-change: transform;
}
/* 图片加载错误样式 */
.img-error {
  background-color: #f5f5f5;
  position: relative;
  overflow: hidden;
}
.img-error::before {
  content: "\e622"; /* 使用图片图标 */
  font-family: "iconfont";
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  font-size: 32px;
  color: #ccc;
}
